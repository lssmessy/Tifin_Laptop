<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tifinnearme.priteshpatel.tifinnearme">

    <permission
        android:name="com.tifinnearme.priteshpatel.tifinnearme.permission.MAP_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.tifinnearme.priteshpatel.tifinnearme.permission.MAP_RECEIVE" />

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- The following two permissions are not required to use
         Google Maps Android API v2, but are recommended. -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!--for push notifications-->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <permission android:name="com.tifinnearme.priteshpatel.tifinnearme.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.tifinnearme.priteshpatel.tifinnearme.permission.C2D_MESSAGE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />
    <!--to render map on App-->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tifin"
        android:label="@string/app_name"

        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCa7GPXboqCgjGLm8hp7a_UhEqGRgBXPAY"/>
        <!--Office key-->
        <!--<meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCo1SkZ9HKthYGy5uZedd8rwrOWo48Jibc" />-->
        <!--Laptop key-->

        <receiver
            android:name=".pushnotifications.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.tifinnearme.priteshpatel.tifinnearme" />
            </intent-filter>
        </receiver>
        <service android:name=".pushnotifications.GcmIntentService" />

        <!--<activity
            android:name=".pushnotifications.DemoActivity"
            android:label="DemoActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>-->

        <activity
            android:name=".login.Starting_page"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signup.Customer_reg"
            android:label="Customer Registration">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.Cutomer_reg" />

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>
        <activity
            android:name=".signup.Tifinvala_reg"
            android:label="Tifinvala Registration">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.Tifinvala_reg" />

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>
        <activity
            android:name=".signup.SignUp_page"
            android:label="Sign Up">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.SignUp_page" />

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>
        <activity
            android:name=".Main_Map"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.Tifinvala_reg" />

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>
        <activity
            android:name=".Webcall"
            android:label="Sign Up">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.Webcall" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.MainActivity" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Subactivity"
            android:label="Sign Up">
            <intent-filter>
                <action android:name="com.tifinnearme.priteshpatel.tifinnearme.Subactivity" />
            </intent-filter>
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>
